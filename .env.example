# Anthropic API Key
ANTHROPIC_API_KEY=your_api_key_here

# QRZ.com Credentials (for callsign lookup and authentication)
# Get an account at https://www.qrz.com/
# Username and password are REQUIRED
QRZ_USERNAME=your_qrz_username
QRZ_PASSWORD=your_qrz_password

# QRZ API Key (optional - for XML Logbook subscription features)
# Only needed if you have a paid subscription
# QRZ_API_KEY=your_qrz_api_key

# Logging
LOG_LEVEL=INFO

# Optional: Override config file location
# CONFIG_PATH=config/config.yaml
