# Anthropic API Key
ANTHROPIC_API_KEY=your_api_key_here

# QRZ.com API Credentials (for callsign lookup and authentication)
# Option 1: API Key (recommended - requires XML Logbook subscription)
# Get your API key from https://www.qrz.com/docs/xml
QRZ_API_KEY=your_qrz_api_key

# Option 2: Username/Password (free account)
# Get an account at https://www.qrz.com/
# QRZ_USERNAME=your_qrz_username
# QRZ_PASSWORD=your_qrz_password

# Logging
LOG_LEVEL=INFO

# Optional: Override config file location
# CONFIG_PATH=config/config.yaml
